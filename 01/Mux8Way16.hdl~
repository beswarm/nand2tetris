/**
 * 8-way 16-bit multiplexor.  
 * out = a if sel == 000
 *       b if sel == 001
 *       etc.
 *       h if sel == 111
 */


CHIP Mux8Way16 {
    IN a[16], b[16], c[16], d[16],
       e[16], f[16], g[16], h[16],
       sel[3];
    OUT out[16];

    PARTS:
    // Put your code here. kangqh
    Mux4Way16(a=a[16],b=c[16],c=e[16],d=g[16],sel=sel[1..2],out=out1);
    Mux4Way16(a=b[16],b=d[16],c=f[16],d=h[16],sel=sel[1..2],out=out2);
    
    Mux16(a=out1,b=out2,sel=sel[0],out=out);
    
}
